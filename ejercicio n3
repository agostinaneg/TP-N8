#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Cantidad: ";
    cin >> n;

    int v[n];
    for (int i = 0; i < n; i++) {
        cout << "Valor " << i << ": ";
        cin >> v[i];
    }

    for (int i = 0; i < n; i++) {
        v[i] = v[i] * 2;
    }

    cout << "Vector duplicado:\n";
    for (int i = 0; i < n; i++) {
        cout << v[i] << " ";
    }
    cout << endl; // Agrego un salto de línea al final para mejor presentación

    return 0;
}
